  <h1>All Contacts</h1>

  <ul>
  <% $rolodex.contacts.each do |contact| %>
    <li><%= contact.first_name %>, 
        <%= contact.last_name %>, 
        <%= contact.email %>, 
        <%= contact.notes %>
        <button><a href="/contacts/<%= contact.id %>">Show Contact</a></button>
    </li>
  <% end %>
  </ul>
